.title{
  height: 200rpx;
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}
.textBox{
  height: 100rpx;
  width: 1000rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.smallTitle{
  font-size: 36rpx;
  margin-right: 40rpx;
}
.textOut{
  height: 60rpx;
  line-height: 60rpx;
  font-size: 34rpx;
  text-indent: 25rpx;
  letter-spacing: 25rpx;
  width: 70%;
  background-color: rgb(248, 248, 248);
  border: solid 1rpx black;
  border-radius: 10rpx;
}

.tabLine{
  height: 80rpx;
  width: 100%;
  display: flex;
  align-items: center;
  border-bottom: solid 1rpx black;
  border-top: solid 1rpx black;
}
.tabs{
  width: 100%;
  height: 80rpx;
  position: relative;
  display: flex;
  justify-content: space-around;
  white-space: nowrap;
}
.tabItems{
  color: rgb(73, 73, 73);
  margin: 10rpx 0;
  height: 60rpx;
  font-size: 35rpx;
  line-height: 60rpx;
  text-align: center;
}
.active{
  border-bottom: solid rgb(51, 85, 148) 7rpx;
  color: rgb(51, 85, 148);
  font-weight: 600;
}

map{
  width: 100%;
  height: 55vh;
}